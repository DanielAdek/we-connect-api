{
  "name": "biz-connect",
  "version": "1.0.0",
  "description": "Get business around you",
  "main": "app.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "start": "babel-node index",
    "start:dev": "nodemon --exec babel-node index",
    "test": "NODE_ENV=test nyc mocha tests/**.spec.js --require @babel/register --exit",
    "test:dev": "npm run posttest && npm test",
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "heroku-postbuild": "npm run migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielAdek/we-connect-api.git"
  },
  "keywords": [
    "Business"
  ],
  "author": "Daniel Adek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielAdek/biz-connect/issues"
  },
  "homepage": "https://github.com/DanielAdek/biz-connect#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.19.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-my-simple-validation": "^0.0.11",
    "jsend": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0"
  }
}
